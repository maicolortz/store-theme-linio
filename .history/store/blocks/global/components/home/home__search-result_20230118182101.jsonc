{
  "search-result-layout.customQuery#home": {
    "title":"search result",
    "props": {
      "querySchema": {
        "skusFilter": "FIRST_AVAILABLE",
        "simulationBehavior": "default"
      }
    },
    "blocks": [
      "search-result-layout.desktop",
      "search-result-layout.mobile",
      "search-not-found-layout"
    ]
  },
  "search-result-layout.desktop": {
    "title":"search result desktop",
    "children": [
      "rich-text#test"
    ],
    "props": {
      "preventRouteChange": true
    }
  },
  "rich-text#test":{
    "title":"text result"
  },
  "search-result-layout.mobile": {
    "title":"search result mobile",
    "children": [
      "flex-layout.col#home__result"
    ]
  },
  "flex-layout.col#home__result": {
    "title":"search result columna",
    "children": [
     /*  "flex-layout.col#home__result--filter", */
      "flex-layout.col#home__result--search"
    ]
  },
/*   "flex-layout.col#home__result--filter":{
    "props":{
"width":"30%"
    }
  }, */
  "flex-layout.col#home__result--search":{
    "title":"flex search result",
   "children":["search-content#home__result--search"]
  },
  "search-content#home__result--search":{
    "title":"flex search result",
    "blocks":["gallery", "not-found"]
  },
  "gallery":{
    "blocks":["product-summary.shelf"]
  },

  "search-not-found-layout": {"title":"search result not found"},
  "slider-layout#text-test": {
    "title":"slider result",
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel"
    },
    "children": ["gallery"]
  }

}